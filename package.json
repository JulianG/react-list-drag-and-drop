{
  "name": "react-list-drag-and-drop",
  "version": "0.10.2",
  "author": "JulianG",
  "license": "MIT",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/JulianG/react-list-drag-and-drop.git"
  },
  "keywords": [
    "react",
    "typescript",
    "drag and drop"
  ],
  "main": "src/index.tsx",
  "scripts": {
    "declaration:build": "rm -f index.d.ts && rm -rfd build/dist && tsc --declaration --declarationMap --pretty -p declaration.tsconfig.json",
    "declaration:check": "git diff --exit-code -- index.d.ts",
    "prepublishOnly": "npm run --silent declaration:build && npm run --silent declaration:check",
    "build": "react-scripts-ts build",
    "start": "react-scripts-ts start",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-lib": "rm -rf ./lib/* && node ./build-lib.js",
    "prepack": "npm run-script build-lib",
    "postversion": "npm run-script build-lib"
  },
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "browserslist": "^4.16.3",
    "eslint": "^7.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-list-drag-and-drop": "^0.9.2",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.13.10",
    "babel-runtime": "^6.23.0",
    "eslint": "^7.22.0",
    "jest": "22.0.1",
    "ts-jest": "22.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "glob": "^7.1.6",
    "install": "^0.13.0",
    "react-scripts-ts": "^3.1.0",
    "why-did-you-update": "^1.0.8"
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "stypes": [
    "./lib/RLDD.d.ts",
    "./lib/RLDDFloatingItemComponent.d.ts",
    "./lib/RLDDItemComponent.d.ts",
    "./lib/RLDDLogic.d.ts"
  ],
  "browserslist": [
    "defaults",
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "publishConfig": {
    "registry": "https://nexus.tasmot.com/repository/TasMot_Repo/"
  }
